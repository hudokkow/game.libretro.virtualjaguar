<?xml version="1.0" encoding="UTF-8"?>
<addon id="game.libretro.virtualjaguar"
		name="Jaguar (virtualjaguar)"
		version="2.1.0"
		provider-name="Libretro">
	<requires>
		<import addon="game.libretro" version="1.0.0"/>
	</requires>
	<extension point="kodi.gameclient"
			library_@PLATFORM@="@LIBRARY_FILENAME@">
		<platforms>Jaguar</platforms>
		<extensions>jag</extensions>
		<supports_vfs>true</supports_vfs>
		<supports_no_game>false</supports_no_game>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">VirtualJaguar v2.1.0</summary>
		<description>Source: https://github.com/libretro/virtualjaguar-libretro</description>
		<platform>@PLATFORM@</platform>
	</extension>
</addon>
